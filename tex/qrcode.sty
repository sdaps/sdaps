\def\@fivezeros{11111}%
\def\@twoones{11}%
\def\@twozeros{00}%
\def\@finderA{00001011101}%
\def\@finderB{10111010000}%
\def\@finderB@three{1011101000}%
\def\@finderB@two{101110100}%
\def\@finderB@one{10111010}%
\def\@finderB@zero{1011101}%
\newif\ifstringoffive
